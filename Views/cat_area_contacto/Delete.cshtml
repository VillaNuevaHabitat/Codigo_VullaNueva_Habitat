@model VillaNueva_Habitat.Models.cat_area_contacto

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>
<div class="card">
    <div class="card-header">
        
    </div>
    <div class="card-body">


        <form asp-action="Eliminar" asp-controller="cat_area_contactoController" method="post">

            <input asp-for="Id" type="hidden" class="form-control">

            <div class="alert alert-danger" role="alert">
                ¿Desea eliminar el contacto : @Html.DisplayTextFor(model => model.id) 
            </div>

            <button type="submit" class="btn btn-danger">Elimnar</button>
            <a asp-action="Index" asp-controller="cat_area_contactoController" class="btn btn-warning">Volver a la lista</a>
        </form>


    </div>
</div>
